<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="pa_scripts">
    <pa_scripts action="INSERT_OR_UPDATE">
        <arguments>u_when_needed</arguments>
        <description>Extract the day of the month from the When needed date.</description>
        <name>NeedIt.day</name>
        <script><![CDATA[whichDay = function(){
  
    //Crear un objeto whenNeeded de la clase GlideDateTime. El valor
    //de whenNeeded es la fecha **When needed** del registro NeedIt.
    var whenNeeded = new GlideDateTime(current.u_when_needed);
    
    //Devolver el dÃ­a del mes del objeto whenNeeded
    return whenNeeded.getDayOfMonthLocalTime();
    
}; 
  
whichDay();
]]></script>
